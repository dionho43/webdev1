<div class="jumbotron">
  <div class="container">





  <% if !signed_in? %>
		<h1>Welcome</h1>
		<h4>You need to be logged in to view your account<h4>
			<li><%=link_to "Register", new_user_path %></li>
			<li><%=link_to "Sign In", login_path %></li>
			
		<% else %>
			<h1>     Welcome <%= @current_user.username %></h1>
			<table>
				<thead>
					<tr>
						<th>Username</th>
						<th>Address</th>
						<th>Phonenumber</th>
						<th>Email</th>
						<th>Password</th>
						<th colspan="3"></th>
					</tr>
				</thead>
				
				<tbody>
					
					<tr>
						<td><%= @current_user.username %></td>
						<td><%= @current_user.address %></td>
						<td><%= @current_user.phoneNumber %></td>
						<td><%= @current_user.email %></td>
						<td><%= @current_user.password %></td>
					</tr>
					
				</tbody>

			</table>
			<li><%=link_to "View my items", pages_myitems_path%></li>
            <li><%=link_to "Edit Account", edit_user_path(@current_user)%></li>
		<%end%>

<br>

  </div>
</div>